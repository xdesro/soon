    "concurrently": "^5.3.0",
    "contentful": "^8.1.7",
    "core-js": "^3.8.2",
    "dotenv": "^8.2.0",
    "gsap": "^3.6.0",
    "gulp": "^4.0.2",
    "gulp-sass": "^4.1.0",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^7.0.0",
    "markdown-it-attrs": "^4.0.0",
    "markdown-it-implicit-figures": "^0.10.0",
    "markdown-it-prism": "^2.1.4",
    "node-fetch": "^2.6.1",
    "nuxt": "^2.14.12",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.1",
    "splitting": "^1.0.6",
    "three": "^0.131.3"
  },
  "devDependencies": {
