{
  "name": "ss21-nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "ELEVENTY_EXPERIMENTAL=true concurrently 'sass --no-source-map --watch ./src/scss:./dist/css' 'rollup --config --watch' 'npx @11ty/eleventy --serve'",
